<Section collapsed="true" MultipleChoiceAnswerIndex="-1" MultipleChoiceRandomizeChoices="false" TrueFalseAnswerIndex="-1" EssayAnswerRows="5" EssayAnswerColumns="60"><Title>
<Text-field style="Heading 2" layout="Heading 2">Burgers Equation</Text-field></Title>
<Group labelreference="L14" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">eq:={diff(u(x,t),t)+u(x,t)*v(x,t)-diff(v(x,t),x),diff(u(x,t),x)-v(x,t)};</Text-field>
</Input>
</Group>
<Group labelreference="L15" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">s1:=liesymmetries(eq,[u(x,t),v(x,t)]);</Text-field>
</Input>
</Group>
<Group labelreference="L16" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">invariant_sol(eq,[u(x,t),v(x,t)],{s1[1][1]});</Text-field>
</Input>
</Group>
<Group labelreference="L18" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">invariant_sol(eq,[u(x,t),v(x,t)],{s1[1][2]});</Text-field>
</Input>
</Group>
<Group labelreference="L19" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">invariant_sol(eq,[u(x,t),v(x,t)],{s1[1][3]});</Text-field>
</Input>
</Group>
<Group labelreference="L20" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">invariant_sol(eq,[u(x,t),v(x,t)],{s1[1][4]});</Text-field>
</Input>
</Group>
<Group labelreference="L21" drawlabel="true">
<Input>
<Text-field prompt="&gt; " style="Maple Input" layout="Normal">s2:=invariant_sol(eq,[u(x,t),v(x,t)],{s1[1][5]});</Text-field>
</Input>
</Group>
